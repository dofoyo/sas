<?xml version="1.0" encoding="GB2312"?>
<root>
	<key name="encoding">GB2312</key>
	<key name="packagePrefix">com.rhb.sas</key>
	<key name="sourcePrefix">com/rhb/sas</key>
	<key name="catalog" value="sas"/>
	<key name="schema">dbo</key>

	<object name="stockholder">
		<field name="pk"	type="String" 	length="24"	caption="编码" firstPk="true"></field>
	    <field name="stockNo"	type="String" 	length="24"	caption="股票代码"></field>
		<field name="stockName" 	type="String" 	length="24"	caption="股票名称"></field>
	    <field name="holderNo"	type="String" 	length="250"	caption="股东代码" ></field>
		<field name="holderName" 	type="String" 	length="250"	caption="股东名称"></field>
		<field name="reportedDate" 	type="java.util.Date" 	length="10"	caption="公告日期"></field>
		<field name="endDate" 	type="java.util.Date" 	length="10"	caption="截止日期"></field>
		<field name="holdingNumber" 	type="Long" 	length="24"	caption="持股数量"></field>
		<field name="holdingRatio" 	type="Double" 	length="24"	caption="持股比例"></field>
		<field name="holdingType" 	type="String" 	length="60"	caption="持股性质"></field>
	</object>
</root>